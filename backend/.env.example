# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key

# Redis (Optional - for background job queue)
UPSTASH_REDIS_URL=

# Azure OpenAI Configuration
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_KEY=your-azure-openai-key
AZURE_OPENAI_DEPLOYMENT=gpt-4
AZURE_OPENAI_DEPLOYMENT_MINI=gpt-4o-mini
AZURE_OPENAI_DEPLOYMENT_GPT4=gpt-4
AZURE_OPENAI_API_VERSION=2024-08-01-preview

# PayPal Configuration
# Get these from: https://developer.paypal.com/dashboard/applications
PAYPAL_CLIENT_ID=your-paypal-client-id
PAYPAL_SECRET=your-paypal-secret
PAYPAL_BASE_URL=https://api-m.sandbox.paypal.com
PAYPAL_WEBHOOK_ID=your-webhook-id

# For Production, use:
# PAYPAL_BASE_URL=https://api-m.paypal.com

# CORS Origins (JSON array string)
CORS_ORIGINS=["http://localhost:5173","http://localhost:8080","https://tailorjob.vercel.app"]

# Matcher Version Control
USE_MATCHER_V5=True
USE_MATCHER_V4=False
USE_MATCHER_V3=False